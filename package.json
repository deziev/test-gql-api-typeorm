    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.19",
    "tsconfig-paths": "3.9.0",
    "type-graphql": "1.1.0",
    "typedi": "0.8.0",
    "typeorm": "0.2.29",
    "typescript": "4.0.5"
  },
  "devDependencies": {
    "nodemon": "2.0.6",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2"
